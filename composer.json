{
    "name": "flaack/laravel-make-module",
    "type": "library",
    "license": "MIT",
    "description": "Provides an artisan make:module command",
    "authors": [
        {
            "name": "Matt J. Sorenson",
            "email": "mjs@emjayess.rocks"
        }
    ],
    "autoload": {
